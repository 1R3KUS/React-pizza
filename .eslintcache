ry = useCallback((index) => {\r\n    dispatch(setCategory(index));\r\n  }, []);\r\n\r\n  const onSelectSortType = useCallback((type) => {\r\n    dispatch(setSortBy(type));\r\n  }, []);\r\n\r\n  const handleAddPizzaToCart = (obj) => {\r\n    dispatch(addPizzaToCart(obj));\r\n  };\r\n\r\n  return (\r\n    <div className=\"container\">\r\n      <div className=\"content__top\">\r\n        <Categories\r\n          activeCategoryIndex={category}\r\n          onClickCategory={onSelectCategory}\r\n          items={categoryNames}\r\n        />\r\n        <SortPopup\r\n          activeSortType={sortBy.type}\r\n          items={sortItems}\r\n          onClickSortType={onSelectSortType}\r\n        />\r\n      </div>\r\n      <h2 className=\"content__title\">Все пиццы</h2>\r\n      <div className=\"content__items\">\r\n        {isLoaded\r\n          ? items.map((obj) => (\r\n              <PizzaBlock\r\n                onClickAddPizza={handleAddPizzaToCart}\r\n                key={obj.id}\r\n                addedCount={cartItems[obj.id] && cartItems[obj.id].items.length}\r\n                {...obj}\r\n              />\r\n            ))\r\n          : Array(10)\r\n              .fill(0)\r\n              .map((_, index) => <PizzaLoadingBlock key={index} />)}\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default Home;\r\n","D:\\prog\\react-lessons\\react-pizza\\src\\pages\\index.js",[],"D:\\prog\\react-lessons\\react-pizza\\src\\pages\\Cart.jsx",[],"D:\\prog\\react-lessons\\react-pizza\\src\\redux\\store.js",[],"D:\\prog\\react-lessons\\react-pizza\\src\\redux\\reducers\\filters.js",[],"D:\\prog\\react-lessons\\react-pizza\\src\\redux\\reducers\\pizzas.js",[],"D:\\prog\\react-lessons\\react-pizza\\src\\redux\\reducers\\index.js",[],"D:\\prog\\react-lessons\\react-pizza\\src\\redux\\actions\\pizzas.js",[],"D:\\prog\\react-lessons\\react-pizza\\src\\redux\\actions\\filters.js",[],["98","99"],"D:\\prog\\react-lessons\\react-pizza\\src\\components\\PizzaBlock\\index.jsx",[],